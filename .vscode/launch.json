{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "python": "${workspaceFolder}/venv/bin/python",
            "envFile": "${workspaceFolder}/.env",
            "jinja": true
        }
    ]
}